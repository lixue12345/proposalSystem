@model IEnumerable<ProposalSystem.Models.Result>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Done</title>
</head>
<body>
    <table class="table table-bordered">
        <caption>已处理提案</caption>
        <thead>
            <tr>
                <th>标题</th>
                <th>类型</th>
                <th>主提案人</th>
                <th>时间</th>
                <th>状态</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                       
                <tr>
                    <td>@Html.DisplayFor(modelItem => item.Proposal1.Title)</td>
                    <td>@Html.DisplayFor(modelItem => item.Proposal1.ProposalType.Type)</td>
                    <td>@Html.DisplayFor(modelItem => item.Proposal1.User.UserName)</td>
                    <td>@Html.DisplayFor(modelItem => item.Date)</td>
                    <td>@Html.DisplayFor(modelItem => item.result1)</td>
                    <td>@Html.ActionLink("进入评价", "Evaluate", new { id=item.ID})</td>
                </tr>
        }
        </tbody>
    </table>
</body>
</html>
