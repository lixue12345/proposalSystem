@model IEnumerable<ProposalSystem.Models.Proposal>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Form</title>

</head>
<body>
    <table class="table table-bordered">
        <caption>提案列表</caption>
        <thead>
            <tr>
                <th>标题</th>
                <th>类型</th>
                <th>主提案人</th>
                <th>时间</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            { 
                <tr>
                    <td>@Html.DisplayFor(modelItem => item.Title)</td>
                    <td>@Html.DisplayFor(modelItem => item.ProposalType.Type)</td>
                    <td>@Html.DisplayFor(modelItem => item.User.UserName)</td>
                    <td>@Html.DisplayFor(modelItem => item.Date)</td>
                    <td>@Html.ActionLink("进入审核", "Details", new { id=item.ID})</td>

                </tr>
        }
        </tbody>
    </table>

</body>
</html>

